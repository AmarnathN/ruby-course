<%= form_for @user do | f |%>
  <table class="table table-striped">
    <tr>
      <td><%= f.label :name%></td>
      <td><%= f.text_field :name%></td>
    </tr>
    <tr>
      <td><%= f.label :email%></td>
      <td><%= f.text_field :email%></td>
    </tr>
    <tr>
      <td><%= f.submit "Update", :class=>"btn btn-primary"%></td>
      </tr>
  </table>
<%end%>
