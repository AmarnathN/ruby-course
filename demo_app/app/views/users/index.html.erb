<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Email</th>
    </tr>
    <% @users.each do | i| %>
      <tr>
        <td><%= i.name %></td>
        <td><%= i.email %></td>
        <td><%= link_to "Show", user_path(i.id) %></td>
        <td><%= link_to "Delete", i, method: :delete, data: {confirm: "Are you sure?"} %></td>
      </tr>
      <%end%>
</table>


<%= link_to "Add User", new_user_path%><br/>
<%= link_to "Upload File", upload_image_users_path%>
